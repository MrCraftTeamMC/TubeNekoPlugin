  "authors": [
    "TubeCraftBuildTeamMC"
  ],
  "contributors": [
    "Block_limr267",
    "Mr_zmh5",
    "SnowCutieOwO"
  ],
  "contact": {
    "homepage": "https://tcbuildmc.xyz",
